{
  "name": "corybuecker",
  "version": "1.0.0",
  "scripts": {
    "build": "npx ts-node-esm src/index.ts",
    "build:esbuild": "npx esbuild --sourcemap --format=esm assets/js/* --outdir=output/js",
    "build:tailwind": "npx tailwind -c tailwind.config.cjs -i assets/css/app.css -o output/css/main.css",
    "format": "npx prettier -w .",
    "lint": "npx eslint ."
  },
  "private": true,
  "dependencies": {
    "@types/marked": "^4.0.8",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "esbuild": "^0.17.6",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "handlebars": "^4.7.7",
    "marked": "^4.2.12",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "yaml": "^2.2.1"
  },
  "type": "module"
}
